!
frr version 7.2.1
frr defaults traditional
hostname gw
log syslog informational
service integrated-vtysh-config
!
interface eth0
 ip address 193.81.7.1/24
!
interface eth1
 ip address 10.0.0.1/24
 ipv6 address 10::1/64
!
interface eth2
 ip address 10.0.6.1/24
 ipv6 address 2001::1/64
!
router-id 193.81.7.1
!
router bgp 2200
 bgp router-id 193.81.7.1
 neighbor 10.0.0.2 remote-as 555
 neighbor 10.0.6.2 remote-as 61486
 !
 address-family ipv4 unicast
  network 193.81.7.0/24
  redistribute connected
  neighbor 10.0.0.2 soft-reconfiguration inbound
  neighbor 10.0.6.2 soft-reconfiguration inbound
 exit-address-family
!
router ospf
 ospf router-id 193.81.7.1
 network 10.0.0.0/24 area 0
 network 10.0.6.0/24 area 0
 network 193.81.7.0/24 area 0
!
line vty
!
end
