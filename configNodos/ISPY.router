!
frr version 7.2.1
frr defaults traditional
hostname ISPY
log syslog informational
service integrated-vtysh-config
!
interface eth0
 ip address 10.0.14.2/24
!
interface eth1
 ip address 10.0.13.1/24
 ip address 110.20.0.1/24
 ipv6 address 10:13::1/64
!
interface eth2
 ip address 110.20.9.1/24
 ipv6 address 2001:5::1/64
!
router-id 10.0.14.2
!
router bgp 3928
 bgp router-id 10.0.14.2
 neighbor 10.0.14.1 remote-as 444
 neighbor 110.20.0.2 remote-as 3928
 !
 address-family ipv4 unicast
  network 110.20.0.0/16
  neighbor 10.0.14.1 soft-reconfiguration inbound
  neighbor 10.0.14.1 route-map nontransit out
  neighbor 110.20.0.2 soft-reconfiguration inbound
 exit-address-family
!
router ospf
 ospf router-id 10.0.14.2
 network 10.0.14.0/24 area 0
 network 110.20.0.0/24 area 0
 network 110.20.9.0/24 area 0
!
bgp as-path access-list 1 permit ^$
bgp as-path access-list 1 deny .*
!
route-map nontransit permit 10
 match as-path 1
!
line vty
!
end
