!
!
service integrated-vtysh-config
!
interface eth0
 ip address 10.0.0.2/24
 ipv6 nd suppress-ra
!
interface eth1
 ip address 10.0.1.1/24
 ipv6 nd suppress-ra
!
interface eth2
 ip address 10.0.3.1/24
 ipv6 nd suppress-ra
!
interface gre0
 ipv6 nd suppress-ra
!
interface gretap0
 ipv6 nd suppress-ra
!
interface lo
!
router bgp 2200
 bgp router-id 10.0.3.1
 network 10.0.0.0/24
 network 10.0.1.0/24
 network 10.0.3.0/24
 network 193.81.0.0/16
 neighbor 10.0.1.2 remote-as 256
 neighbor 10.0.1.2 soft-reconfiguration inbound
!
router ospf
 ospf router-id 10.0.0.2
 passive-interface eth1
 passive-interface eth2
 network 10.0.0.0/24 area 0.0.0.0
 network 10.0.1.0/24 area 0.0.0.0
 network 10.0.3.0/24 area 0.0.0.0
!
ip route 0.0.0.0/0 10.0.1.2
!
ip forwarding
ipv6 forwarding
!
line vty
!
end
